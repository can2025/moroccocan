{
  "expo": {
    "name": "CAN 2025",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/logocan1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Stay up to date with AFCON 2025 (Coupe d'Afrique des Nations) in Morocco. Get the latest fixtures, results, team news, standings, and highlights from Africa's biggest football tournament.",  
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logocan520x520.png"
    },
    "android": {
      "package": "com.afcon2025.morocco",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logocan520x520.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "ios": {
      "bundleIdentifier": "com.can2025.morocco",
      "buildNumber": "1.0.6",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app may access the camera through built-in features or third-party libraries."
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "37e98109-02ac-4b3e-be4b-7528ec64f39b"
      }
    },
    "owner": "outhman"
  }
}
